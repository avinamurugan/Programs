#include<stdio.h>
int main(){
int arr[100],n,i,key,position;
printf("enter the size");
scanf("%d",&n);
printf("position");
scanf("%d",&position);
printf("array element");
for(i=0;i<n;i++){

	scanf("%d\n",&arr[i]);
}
for(i=0;i<n;i++){
	if(i>=position){
		arr[i]=arr[i-1];
}
}
arr[n-1]=0;
for(i=0;i<n;i++){

printf("%d",arr[i]);
}
}
